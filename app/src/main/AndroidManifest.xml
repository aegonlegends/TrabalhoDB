        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.ActivitySelectPeca"
            android:label="@string/app_name" />
        <activity android:name=".Activity.ActivityEditPeca" />
        <activity android:name=".Activity.ActivitySelectProprietario" />
        <activity android:name=".Activity.ActivityEditProprietario" />
        <activity android:name=".Activity.ActivityEditServico" />
